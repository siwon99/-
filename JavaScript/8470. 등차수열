let num1 =[3, 5, 7] //33
let num2 = [1, 2, 2] //3

function solution1 (num1) {
    let ans = num1[0] + (num1[2] - 1) * num1[1]
    console.log("N번째 항", ans);
    return ans;
}

solution1(num1)

function solution2 (num2) {
    let ans = num2[0] + (num2[2] - 1) * num2[1]
    console.log("N번째 항", ans);
    return ans;
}

solution2(num2)
